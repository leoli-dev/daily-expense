<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}
                Daily Expense
                {% if pageTitle is defined %}| {{ pageTitle }}{% endif %}
            {% endblock %}
        </title>
        <link rel="icon" type="image/png" sizes="32x32" href="favicon.png">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body
        {% if bodyId is defined %}id="{{ bodyId }}"{% endif %}
        {% if bodyClass is defined %}class="{{ bodyClass }}"{% endif %}
    >
        {% block main %}{% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
